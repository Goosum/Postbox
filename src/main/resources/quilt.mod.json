{
	"schema_version": 1,
	"quilt_loader": {
		"group": "io.github.goosum",
		"id": "postbox",
		"version": "${version}",
		"metadata": {
			"name": "Postbox",
			"description": "A simple mod for delivering packages to your friends.",
			"contributors": {
				"Your name here": "Goosum"
			},
			"contact": {
				"issues": "https://github.com/Goosum/Postbox/issues",
				"sources": "https://github.com/Goosum/Postbox"
			},
			"icon": "assets/example_mod/icon.png",
			"license": "MIT"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "io.github.goosum.postbox.Postbox",
			"client_init": "io.github.goosum.postbox.client.ClientRegistry"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.19.1"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=7.0.2"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20"
			}
		]
	},
	"mixin": "postbox.mixins.json"
